{
    "prefix": [
        "PREFIX : <http://www.phenome-fppn.fr/vocabulary/m3p/2015/event#> ",
        "PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> ",
        "PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> ",
        "PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> ",
        "PREFIX dcterms: <http://purl.org/dc/terms/> ",
        "PREFIX oa: <http://www.w3.org/ns/oa#> ",
        "PREFIX context: <http://www.phenome-fppn.fr/m3p/events_sowing> ",
        "PREFIX time: <http://www.w3.org/2006/time#> "
    ],
    "uuidPerRow": 3,
    "associativeRules": [
        "context:instant_{uuid_2} rdf:type time:Instant ;",
        "   time:inXSDDateTimeStamp \"{Date event}\"^^xsd:dateTimeStamp ;",
        "   dcterms:created \"{dateNow}\"^^xsd:dateTime .",
        "context:anno_{uuid_2} oa:hasTarget context:event_{uuid_1} ;",
        "   oa:bodyValue \"{Content}\"^^xsd:string .",
        "context:event_{uuid_1} rdf:type :Displacement ;",
        "   time:hasTime context:instant_{uuid_2} ;",
        "   :to <{Platform}> ;",
        "   :concern <{uri}> ."
    ]
}